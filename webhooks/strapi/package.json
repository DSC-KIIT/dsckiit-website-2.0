{
	"name": "strapi-webhook",
	"version": "2.3.0",
	"description": "A webhook-router for outgoing requests from strapi webhook",
	"main": "index.js",
	"scripts": {
		"build": "gulp build",
		"start": "node build/main.js || node build/src/index.js",
		"tsc": "tsc",
		"dev": "NODE_ENV=development ts-node-dev --watch src/**/*.ts src/index.ts",
		"test": "lab --sourcemaps --transform node_modules/lab-transform-typescript **/*.test.ts --timeout 100",
		"testw": "gulp testWatch"
	},
	"keywords": [
		"webhook",
		"strapi",
		"hapi"
	],
	"author": "aditya-mitra",
	"license": "MIT",
	"dependencies": {
		"@hapi/hapi": "^20.1.4",
		"axios": "^0.21.1"
	},
	"devDependencies": {
		"@hapi/lab": "^24.2.1",
		"@types/chai": "^4.2.19",
		"@types/gulp": "^4.0.8",
		"@types/hapi__hapi": "^20.0.8",
		"@types/sinon": "^10.0.2",
		"chai": "^4.3.4",
		"gulp": "^4.0.2",
		"lab-transform-typescript": "^3.0.1",
		"mocha": "^9.0.1",
		"sinon": "^11.1.1",
		"ts-node-dev": "^1.1.6",
		"typescript": "^4.3.4",
		"uglify-js": "^3.13.10",
		"yarn-upgrade-all": "^0.5.4"
	},
	"engines": {
		"node": ">=14.0.0 <=14.x.x",
		"npm": "^7.0.0"
	}
}
